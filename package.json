  "scripts": {
    "clean": "rimraf .next",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:watch": "chokidar 'src/**/*.{ts,tsx}' -c 'pnpm exec eslint --ext .ts,.tsx src --fix || true'",
    "check:types": "pnpm exec tsc --noEmit",
    "check:lint": "pnpm exec eslint 'src/**/*.{ts,tsx}' --ext .ts,.tsx",
    "check:all": "pnpm run check:types && pnpm run check:lint",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --passWithNoTests",
    "test:watch": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --coverage",
    "ui:debug": "node scripts/ui-debug.js",
    "ui:check": "echo 'Run RVOISUIDebug.run() in browser console to check UI issues'",
    "ui:dev": "next dev & echo 'UI Debugging: Run RVOISUIDebug.debugCSS.add() in browser console to add visual debugging outlines'",
    "dev:mobile": "next dev --hostname 0.0.0.0 --port 3000",
    "gen-types": "supabase gen types typescript --project-id zcgbzbviyaqqplpamcbh > ./types/supabase.ts",
    "backup:start": "pnpm ts-node scripts/start-backup-service.ts",
    "backup:manual": "pnpm ts-node scripts/manual-backup.ts",
    "backup:restore": "pnpm ts-node scripts/restore-backup.ts",
    "backup:verify": "pnpm ts-node scripts/verify-backup.ts",
    "vercel:logs": "vercel logs --follow",
    "vercel:inspect": "vercel inspect"
  },
